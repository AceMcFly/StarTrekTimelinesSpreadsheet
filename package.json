{
  "name": "startrektimelinestool",
  "productName": "Star Trek Timelines Crew Management",
  "version": "0.8.1",
  "description": "Tool for managing Star Trek Timelines crew",
  "author": "IAmPicard",
  "copyright": "Copyright Â© 2018 IAmPicard",
  "homepage": "https://github.com/IAmPicard/StarTrekTimelinesSpreadsheet",
  "repository": {
    "type": "git",
    "url": "https://github.com/IAmPicard/StarTrekTimelinesSpreadsheet.git"
  },
  "license": "GPL-3.0",
  "build": {
    "appId": "IAmPicard.StarTrekTimelinesTool",
    "mac": {
      "category": "public.app-category.games"
    }
  },
  "babel": {
    "presets": [
      "@babel/react",
      [
        "@babel/env",
        {
          "targets": {
            "chrome": "66"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "main": "main.js",
  "scripts": {
    "prod": "webpack --mode production --config webpack.build.electron.config.js && electron --noDevServer .",
    "dev": "webpack-dev-server --mode development --hot --host 0.0.0.0 --config=./webpack.dev.electron.config.js",
    "build": "webpack --mode production --config webpack.build.electron.config.js",
    "buildweb": "webpack --mode production --config webpack.build.web.config.js",
    "buildexp": "webpack --mode production --config webpack.build.exp.config.js",
    "bundlestats": "webpack --mode production --config webpack.build.web.config.js --profile --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json",
    "serveweb": "webpack-dev-server --mode development --hot --host localhost --port 80 --config=./webpack.dev.web.config.js",
    "package": "webpack --mode production --config webpack.build.electron.config.js",
    "postpackage": "electron-packager ./ --out=./builds --prune --icon=src/assets/icons/ATFleet.ico",
    "packagedev": "webpack --mode development --config webpack.build.electron.config.js",
    "postpackagedev": "electron-packager ./ --out=./builds --prune --icon=src/assets/icons/ATFleet.ico",
    "installer": "node createinstaller.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "latest",
    "@babel/plugin-proposal-object-rest-spread": "latest",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "latest",
    "@uifabric/styling": "^6.34.0",
    "babel-loader": "latest",
    "css-loader": "^1.0.1",
    "electron": "^3.0.8",
    "electron-builder": "^20.33.2",
    "electron-packager": "latest",
    "electron-rebuild": "latest",
    "favicons-webpack-plugin": "latest",
    "file-loader": "latest",
    "html-webpack-plugin": "latest",
    "ifdef-loader": "latest",
    "mini-css-extract-plugin": "^0.4.4",
    "nan": "latest",
    "office-ui-fabric-react": "^6.100.2",
    "pubnub": "^4.21.6",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.0",
    "tslint": "latest",
    "tslint-config-standard": "latest",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.1.6",
    "url-loader": "^1.1.2",
    "webpack": "4.23.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cdn-plugin": "^2.3.3",
    "webpack-cli": "latest",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "latest",
    "webpack-preprocessor-loader": "latest",
    "webpack-serve": "latest",
    "worker-loader": "latest",
    "xlsx-populate": "latest"
  },
  "dependencies": {
    "bindings": "latest",
    "dexie": "latest",
    "electron-settings": "^3.2.0",
    "fb": "latest",
    "history": "latest",
    "react-device-detect": "^1.6.1",
    "react-table": "latest",
    "semver": "latest",
    "snarkdown": "^1.2.2",
    "stt-native": "file:native",
    "stt-native-wasm": "file:native_wasm",
    "sttapi": "file:STTApi",
    "url-search-params-polyfill": "latest",
    "uuid": "latest"
  }
}
