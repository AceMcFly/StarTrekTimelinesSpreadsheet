    "electron-rebuild": "^1.8.1",
    "file-loader": "latest",
    "html-webpack-plugin": "latest",
    "ifdef-loader": "^2.0.3",
    "mini-css-extract-plugin": "^0.4.1",
    "nan": "latest",
    "react": "latest",
    "react-dom": "latest",
    "style-loader": "latest",
    "ts-loader": "latest",
    "tslint": "latest",
    "tslint-config-standard": "latest",
    "tslint-loader": "latest",
    "typescript": "latest",
    "url-loader": "latest",
    "webpack": "^4.15.1",
    "webpack-cli": "latest",
    "webpack-dev-server": "latest",
    "webpack-merge": "^4.1.3",
    "webpack-preprocessor-loader": "^1.0.4",
    "webpack-serve": "^2.0.2",
    "worker-loader": "latest"
  },
